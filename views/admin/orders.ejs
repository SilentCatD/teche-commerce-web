<!DOCTYPE html>
<html lang="en">

<head>
    <%-include('partials/head.ejs', {title: title}) %> 
    <link rel="stylesheet" href="/admin/css/table_data.css">
    <link rel="stylesheet" href="/admin/css/add_img_carousel.css">
    <link rel="stylesheet" href="/admin/css/form_loading_product.css">
    <!-- <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.0.2/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-EVSTQN3/azprG1Anm3QDgpJLIm9Nao0Yz1ztcQTwFspd3yD65VohhpuuCOmLASjC" crossorigin="anonymous"> -->
</head>

<body>
    <div class="modal" tabindex="-1" id="order-modal">
        <div class="modal-dialog modal-dialog-scrollable modal-dialog-centered">
          <div class="modal-content">
            <div class="modal-header">
              <h5 class="modal-title">Order details</h5>
              <button type="button" class="btn-close" data-bs-dismiss="modal" aria-label="Close"></button>
            </div>
            <div class="modal-body">
              <div class="row">
                    <h5 class="text-info mb-1"> yyUser Info</h3>
                    <div class="form-group">
                        <label class="col-lg-3 control-label">ID</label>
                        <div class="col-lg-8">
                        <input class="form-control" id="orderId" type="text" disabled>
                        </div>
                    </div>
                    <div class="form-group">
                        <label class="col-lg-3 control-label">Created At</label>
                        <div class="col-lg-8">
                        <input class="form-control" id="orderCreatedAt" type="text" disabled>
                        </div>
                    </div>
                    <div class="form-group">
                        <label class="col-lg-3 control-label">Reciever</label>
                        <div class="col-lg-8">
                        <input class="form-control" id="orderReciever" type="text" disabled>
                        </div>
                    </div>
                    <div class="form-group">
                        <label class="col-lg-3 control-label">Address</label>
                        <div class="col-lg-8">
                        <input class="form-control" id="orderAddress" type="text" disabled>
                        </div>
                    </div>
                    <div class="form-group">
                        <label class="col-lg-3 control-label">Phone</label>
                        <div class="col-lg-8">
                            <input class="form-control" id="orderPhone" type="text" disabled>
                        </div>
                    </div>
                    <div class="form-group">
                        <label class="col-lg-3 control-label">Note</label>
                        <div class="col-lg-8">
                            <textarea class="form-control" id="orderNote" type="text" disabled></textarea>
                        </div>
                    </div>
                    <h5 class="text-info mb-1"> Order items</h3>
                    <table class="table table-responsive table-striped" id="orderTable">
                        <thead>
                          <tr>
                            <th scope="col">#</th>
                            <th scope="col">Product Name</th>
                            <th scope="col">Product Price</th>
                            <th scope="col">Ammount</th>
                            <th scope="col">Total</th>
                          </tr>
                        </thead>
                        <tbody>
                          
                        </tbody>
                    </table>
                    <div class="col-md-6 col-sm-12">
                        <select class="form-select form-select-sm"  id="statusSelect">
                           
                        </select>
                    </div>
              </div>
            </div>
            <div class="modal-footer">
                <button type="button" class="btn btn-primary" id="orderSaveChange">Save changes</button>
              <button type="button" class="btn btn-secondary" data-bs-dismiss="modal">Close</button>
            </div>
          </div>
        </div>
      </div>
    <div class="container-xxl position-relative bg-white d-flex p-0">
        <!-- Spinner Start -->
        <%-include('partials/spinner.ejs') %> 
        <!-- Spinner End -->


        <!-- Sidebar Start -->
        <%-include('partials/sidebar.ejs', {active: active}) %> 
        <!-- Sidebar End -->


        <!-- Content Start -->
        <div class="content">
        <!-- Navbar Start -->
        <%-include('partials/navbar.ejs') %> 
        <!-- Navbar End -->

        <!--Alert Start-->
        <%-include('partials/alert.ejs') %>
        <!--Alert End-->

        <!-- Orders Search Start-->
        <div class="container-fluid pt-4 px-4">
            <div class="row bg-light rounded align-items-center justify-content-center mx-0 p-4">
                <div class="col-12">
                    <form class="d-md-flex ms-4 mb-4 w-50 ms-md-0 ms-auto me-auto" id="searchForm">
                        <input class="form-control border-0" type="search" placeholder="Search" id="tableSearch">
                    </form>
                </div>
                <div class="col-md-6 col-sm-12">
                    <select class="form-select form-select-sm" id="sortOption" >
                        <option selected value="createdAt">CreatedAt</option>
                    </select>
                </div>
                <div class="col-md-6 col-sm-12">
                    <select class="form-select form-select-sm"  id="sortOrder">
                        <option selected value="desc">DESC</option>
                        <option value="asc">ASC</option>
                    </select>
                </div>
                <div class="mt-3 col-md-6 ">
                    <select class="form-select" id="stateSelect">
                        <option selected value="">All</option>
                        <option  value="new">New</option>
                        <option  value="processing">Processing</option>
                        <option  value="shipping">Shipping</option>
                        <option  value="completed">Completed</option>
                    </select>
                </div>
            </div>
        </div>
        <!-- Orders Search End-->
  
   
        <!--  Product Table start -->
        <%-include('partials/data_table.ejs') %>
        <!-- Product Table end-->
        <!-- Footer Start -->
        <%-include('partials/footer.ejs') %>
        <!-- Footer End -->
        <!-- Content End -->
        <!-- Back to Top -->
        <%-include('partials/back_to_top_btn.ejs') %>
       
        <!-- JavaScript Libraries -->
        <%-include('partials/include_js.ejs') %> 
        <script src="https://unpkg.com/validator@latest/validator.min.js"></script>
        <script src="https://cdn.jsdelivr.net/npm/axios/dist/axios.min.js"></script>
        <script type="module" src="/admin/js/alert.js"></script>
        <script type="module" src="/admin/js/data_table.js"></script>
        <script type="module" src="/admin/js/sidebar.js"></script>
        <script type="module" src="/admin/js/navbar.js"></script>
        <script type="module" src="/admin/js/orders.js"></script>
</body>

</html>